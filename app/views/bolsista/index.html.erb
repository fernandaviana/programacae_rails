    <td><%= bolsistum.nomebanco %></td>
    <td><%= bolsistum.numeroagencia %></td>
    <td><%= bolsistum.numeroconta %></td>
    <td><%= bolsistum.semestreinicialcurso %></td>
    <td><%= bolsistum.anoinicialcurso %></td>
    <td><%= bolsistum.observacao %></td>
    <td><%= bolsistum.curso %></td>
    <td><%= link_to 'Show', bolsistum %></td>
    <td><%= link_to 'Edit', edit_bolsistum_path(bolsistum) %></td>
    <td><%= link_to 'Destroy', bolsistum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

