<% @bolsista.each do |bolsistum| %>
  <tr>
    <td><%= bolsistum.matricula %></td>
    <td><%= bolsistum.numeropasta %></td>
    <td><%= bolsistum.nomebolsista %></td>
    <td><%= bolsistum.endereco %></td>
    <td><%= bolsistum.bairro %></td>
    <td><%= bolsistum.cidade %></td>
    <td><%= bolsistum.estado %></td>
    <td><%= bolsistum.telefoneresidencial %></td>
    <td><%= bolsistum.telefonecelular %></td>
    <td><%= bolsistum.email %></td>
    <td><%= bolsistum.cpf %></td>
    <td><%= bolsistum.rg %></td>
